			fieldset
				h2(class="fs-title selmecAzul") <span data-i18n="demo.inspdiaria_form.dados_gerais"></span>
					.row
						.input-field.col.s12.m6
							i.material-icons.prefix.selmecAzul person
							label(for="operator") checked by
							input(id="operator", autocomplete="off",readonly, type="text", name="operator", value="#{Site? Site[0].operator:DataU.nome}")
						.input-field.col.s12.m6
							i.material-icons.prefix.selmecAzul note
							label(for="company") Company
							input.preencher(type="text", id="company", readonly,name="company", value="COMSERV")
						.input-field.col.s12.m6
							i.material-icons.prefix.selmecAzul date_range
							label(for="dateChecked") <span data-i18n="demo.inspdiaria_form.data_hora"></span>
							input.preencher(type="text", id="dateChecked", readonly,name="dateChecked", value="#{Site? Site[0].dateChecked : ((new Date()).getDate()<10? '0'+(new Date()).getDate():(new Date()).getDate())+'/'+(((new Date()).getMonth()+1)<10? ('0'+((new Date()).getMonth()+1)):((new Date()).getMonth()+1))+'/'+((new Date()).getFullYear())+'   '+((new Date()).getHours()<10? ('0'+(new Date()).getHours()): (new Date()).getHours() )+' : '+((new Date()).getMinutes()<10? ('0'+(new Date()).getMinutes()):(new Date()).getMinutes())}")
						.input-field.col.s12.m6
							i.material-icons.prefix.selmecAzul edit
							label(for="checkedBy") ckecked by
							input.preencher(type="text", id="checkedBy",  name="checkedBy", value="#{Site ? Site[0].checkedBy:''}")
				input(type="button", name="next", class="next action-button", value="Next >>")
